{
  "name": "app-seguridad-back",
  "packageManager": "yarn@4.0.1",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register --respawn --clear ./src/main.ts",
    "build": "rimraf ./dist && tsc",
    "deploy": "yarn build && cross-env NODE_ENV=production TZ=America/Lima node ./dist/main.js",
    "lint": "eslint . --ext ts --fix",
    "db:seed": "prisma db seed",
    "db:client": "prisma generate",
    "db:migrate": "prisma migrate deploy && yarn db:client",
    "db:migrate:dev": "prisma migrate dev",
    "postinstall": "cd .. && husky install app-seguridad/.husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/module-alias": "^2.0.3",
    "@types/morgan": "^1.9.8",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "prettier": "^3.0.3",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  },
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "ts-node src/database/seed.ts"
  }
}
