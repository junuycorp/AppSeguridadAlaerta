{
  "name": "@alerta/agente",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register --env-file .env --respawn --clear ./src/main.ts",
    "build": "rimraf ./dist && tsc -p tsconfig.json",
    "start": "cross-env NODE_ENV=production TZ=America/Lima node --env-file .env ./dist/main.js",
    "deploy": "yarn build && yarn start",
    "db:seed": "prisma db seed",
    "db:client": "prisma generate",
    "db:migrate": "prisma migrate deploy && yarn db:client",
    "db:migrate:dev": "prisma migrate dev",
    "dev:agente": "yarn dev",
    "db:dev:agente": "yarn db:migrate:dev",
    "db:client:agente": "yarn db:client",
    "db:deploy:agente": "yarn db:migrate",
    "build:agente": "yarn build",
    "start:agente": "yarn start"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/module-alias": "^2.0.3",
    "@types/morgan": "^1.9.8",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "cross-env": "^7.0.3",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  },
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "ts-node src/database/seed.ts"
  }
}
